          <main id="main" class="site-main" role="main">
      <% if current_user == @user %> 
            <div class="container">
              <div class="row"> 
              <h2> What's up <%= @user.first_name %>! </h2>
                <div class="col-md-6">
                  <section class="widget danish_widget_about">
                    <div class="about-author-container">
                      <img src="<%= @user.avatar %>" alt="<%= @user.first_name %>">
                      <div class="about-author-info">
                        <h2 class="widget-title"><%= @user.full_name %></h2>
                        <span class="author-subtitle"><%= @user.location %></span>
                        <span class="author-subtitle">Age: <%= @user.age %></span>
                        <span class="author-subtitle">Email: <%= @user.email %></span>
                        <div class="author-description">
                          <p><%= @user.bio %></p>
                          <a href="/users/<%= @user.id %>/edit" class="more-link"><span class="moretext">Edit Profile</span></a>
                          <a href="/conversations" class="more-link"><span class="moretext">View Messages</span></a>
                        </div><!-- .author-description -->
                        <div class="author-footer">
                          <div class="author-social">
                            <a href="https://www.facebook.com/Template-Ninja-1555671317979815/" target="_blank"><i class="fa fa-facebook"></i></a>
                            <a href="http://twitter.com/_templateninja" target="_blank"><i class="fa fa-twitter"></i></a>
                            <a href="https://www.instagram.com/templateninja/" target="_blank"><i class="fa fa-instagram"></i></a>
                            <a href="#" target="_blank"><i class="fa fa-youtube"></i></a>
                          </div><!-- .author-social -->
                        </div><!-- .author-footer -->
                      </div><!-- .about-author-info -->
                    </div><!-- .about-author-container -->
                  </section><!-- .danish_widget_about -->
                </div> <!-- col-md-6 -->
                <div class="col-md-6">
                  <section class="widget danish_widget_popular_entries">
                    <h2 class="widget-title">Your upcoming events:</h2>
                    <ul>
                      <li>
                  <%if @upcoming_events.length == 0%>
                        <p><i>No Events</i> </p>
                        <%else%>
                    <% @upcoming_events.each do |upcoming_event| %>
                        <div class="popular-entry-container">
                          <div class="entry-image">
                            <img src="<%=upcoming_event.avatar_file_name%>" alt="/events/<%=upcoming_event.id%>">
                          </div><!-- .entry-image -->
                          <div class="entry-content">
                            <h4 class="entry-title">
                              <a href="/events/<%=upcoming_event.id%>" rel="bookmark"></a>
                            </h4>
                            <span class="entry-category"><a href="/events/<%=upcoming_event.id%>"><%=upcoming_event.title%></a></span>
                            <span class="entry-datetime"><%=upcoming_event.date%></span>
                          </div><!-- .entry-content -->
                        </div><!-- .popular-entry-container -->
                      <%end%>
                  <%end%>
                      </li>
                    </ul>
                  <h2 class="widget-title">Your past events:</h2>
                    <ul>
                      <li>
                  <%if @past_events.length == 0%>
                    <p><i> No Events </i></p>
                  <%else%>
                    <% @past_events.each do |past_event| %>
                        <div class="popular-entry-container">
                          <div class="entry-image">
                            <img src="/images/default.png" alt="/events/<%=past_event.id%>">
                          </div><!-- .entry-image -->
                          <div class="entry-content">
                            <h4 class="entry-title">
                              <a href="/events/<%=past_event.id%>" rel="bookmark"></a>
                            </h4>
                            <span class="entry-category"><a href="/events/<%=past_event.id%>"><%=past_event.title%></a></span>
                            <span class="entry-datetime"><%=past_event.date%></span>
                          </div><!-- .entry-content -->
                        </div><!-- .popular-entry-container -->
                    <%end%>
                  <%end%>
                      </li>
                    </ul>
                <h2 class="widget-title"> **Events that you host**:</h2>
                    <ul>
                      <li>
                  <%if @hosted_events.length == 0%>
                    <p><i> No Events </i></p>
                  <%else%>
                    <% @hosted_events.each do |hosted_event| %>
                        <div class="popular-entry-container">
                          <div class="entry-image">
                            <img src="/images/default.png" alt="/events/<%=hosted_event.id%>">
                          </div><!-- .entry-image -->
                          <div class="entry-content">
                            <h4 class="entry-title">
                              <a href="/events/<%=hosted_event.id%>" rel="bookmark"></a>
                            </h4>
                            <span class="entry-category"><a href="/events/<%=hosted_event.id%>"><%=hosted_event.title%></a></span>
                            <span class="entry-datetime"><%=hosted_event.date%></span>
                          </div><!-- .entry-content -->
                        </div><!-- .popular-entry-container -->
                    <%end%>
                  <%end%>
                      </li>
                    </ul>
                  </section><!-- .danish_widget_popular_entries -->
                </div> <!-- col-md-6 -->
               <section class="widget widget_recent_comments">
