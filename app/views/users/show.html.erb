                        <a href="/events/<%=comment.event_id%>"><span class="comment-author-link"><%=comment.event.title%></span></a> 
                        "<%=comment.body%>"
                      </li>
                    <%end%>
                <%end%>
                    </ul>
                  </section><!-- .widget_recent_comments -->
                </div><!-- .col-md-4 -->
              </div><!-- .row -->
            </div><!-- .container -->
