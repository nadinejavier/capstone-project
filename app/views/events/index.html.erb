<div ng-app="app">
 <div ng-controller="eventsCtrl"  ng-init="setup()">

<h1> All events: </h1>
  <div>
      Filter Events: <input ng-model="titleFilter">
  </div>

      <!-- content -->
      <div id="content" class="site-content">
        <div id="primary" class="content-area">
          <main id="main" class="site-main" role="main">
  <div ng-repeat="event in events | filter: {title: titleFilter} | orderBy: 'date' ">
            <div class="container">
              <div class="row">
                <div class="col-md-8 col-xs-12">
                  <div class="row container-post">
                    <div class="col-sm-12 col-xs-12">
                      <article class="post format-standard hentry">
                        <div class="post-container">
                          <div class="post-content">
                            <img src="{{event.avatar}}" alt="Do not leave your camera">
                            <div class="heading-title heading-small">
                              <h2><a href="/events/{{event.id}}" rel="bookmark"> {{event.title}} </a></h2>
                            </div><!-- .heading-title -->
                            <div class="post-meta">
                              <span class="posted-on"><time class="entry-date" datetime="2016-05-09"> {{event.date | date: format : shortDate}}</time></span>
                            </div><!-- .post-meta -->
                          </div><!-- .post-content -->
                        </div>
                      </article><!-- #post-## -->
                    </div><!-- .col-sm-6 -->
                  </div><!-- .container-post -->
                </div><!-- .col-md-8 -->
              </div><!-- .row -->
            </div><!-- .container -->
            </div> <!-- repeat -->
          </main><!-- #main -->
        </div><!-- #primary -->
      </div><!-- #content -->
</div>
